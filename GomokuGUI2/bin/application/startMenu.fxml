<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.startMenuController">
   <children>
      <VBox prefHeight="500.0" prefWidth="800.0">
         <children>
            <HBox alignment="CENTER" prefHeight="100.0" prefWidth="800.0">
               <children>
                  <Label fx:id="gomokuTitle" alignment="TOP_LEFT" text="Gomoku" textAlignment="RIGHT">
                     <font>
                        <Font name="System Bold" size="28.0" />
                     </font>
                  </Label>
               </children>
            </HBox>
            <VBox alignment="TOP_CENTER" prefHeight="200.0" prefWidth="800.0">
               <children>
                  <Button fx:id="pVsAIButton" alignment="TOP_CENTER" mnemonicParsing="false" onAction="#startPvsAI" prefWidth="130.0" text="Player vs. AI" />
                  <Button fx:id="pVsPButton" mnemonicParsing="false" onAction="#startPVsPlayer" prefWidth="130.0" text="Player vs. Player">
                     <VBox.margin>
                        <Insets top="50.0" />
                     </VBox.margin>
                  </Button>
                  <Button fx:id="quitButton" mnemonicParsing="false" onAction="#quitButton" text="Quit">
                     <VBox.margin>
                        <Insets top="60.0" />
                     </VBox.margin>
                  </Button>
               </children>
            </VBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
