<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="500.0" prefWidth="400.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8" fx:controller="victoryScreen.victoryScreenController">
   <children>
      <VBox prefHeight="500.0" prefWidth="400.0">
         <children>
            <HBox alignment="TOP_CENTER" prefHeight="100.0" prefWidth="200.0">
               <VBox.margin>
                  <Insets top="50.0" />
               </VBox.margin>
               <children>
                  <Label fx:id="victoryLabel" text="Player 1 Wins!">
                     <font>
                        <Font name="System Bold" size="21.0" />
                     </font>
                  </Label>
               </children>
            </HBox>
            <HBox alignment="TOP_CENTER" prefHeight="100.0" prefWidth="200.0">
               <VBox.margin>
                  <Insets top="50.0" />
               </VBox.margin>
               <children>
                  <Button fx:id="quitButton" mnemonicParsing="false" onAction="#quitButtonClicked" text="Quit">
                     <HBox.margin>
                        <Insets left="15.0" top="30.0" />
                     </HBox.margin>
                  </Button>
                  <Button fx:id="playAgainButton" mnemonicParsing="false" onAction="#playAgainButtonClicked" text="Play Again">
                     <HBox.margin>
                        <Insets left="60.0" top="30.0" />
                     </HBox.margin>
                  </Button>
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
